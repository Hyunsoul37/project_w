<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shop.winetoy.server.member.dao.MemberInfoDao">
	<insert id="join" parameterType="shop.winetoy.server.member.entity.MemberInfoVO">
		INSERT INTO `winetoy`.`memberInfo`
		(
			`id`,
			`password`,
			`name`,
			`email`,
			`address`,
			`phone`
		)
		VALUES
		(
			#{id},
			#{password},
			#{name},
			#{email},
			#{address},
			#{phone}
		);
	</insert>
</mapper>